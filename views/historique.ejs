    <ul class="list-group">
      <table class="table table-striped table-sm">
        <thead>
           <tr>
              <th scope="col">Nom - prenom</th>
              <th scope="col">email</th>
              <th scope="col">Boutique</th>
              <th scope="col">date du commande</th>
              <th scope="col">N° Commande</th> 
           
           </tr>
        </thead>
        <tbody>
       
             <tr class="align-middle">
                 <td><%= commandes.name %>  <%= commandes.prenom %> </td>
                 <td> <%= commandes.email %></td>
                 <td><%=commandes.category%></td>
                 <td><%= commandes.createdAt.toLocaleDateString("fr-FR", {
                  year:"numeric",
                  month:"long",
                  day:"numeric"
                 }) %></td>
                   <td><%=commandes._id%></td>
             </tr>
        
         </tbody>
        </table>
        <table class="table table-striped table-sm">
            <thead>
               <tr>
                  <th scope="col">produits</th>
                  <th scope="col">Numero du produit</th>
                  <th scope="col">prix unitaire</th>
                  <th scope="col">Quantite</th> 
               </tr>
            </thead>
            <tbody>
            
                <% commandes.product.forEach(element => { %>
                 <tr class="align-middle">
                     <td><%= element.name %></td>
                     <td><%= element.idProduct %></td>
                     <td><%= element.prix %></td>
                     <td><%= element.qte %></td>
                 </tr>
                <% }) %> 
         
             </tbody>
            </table>
            <%  var total = 0;
            commandes.product.forEach(function(element) {
                total += element.qte * element.prix;
            }); %>
            <b>  Le prix Total: <%= total %> FCFA </b>


            
 
     
 

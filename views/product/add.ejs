<form class="formm" action="/addProduct" method="POST">
 
       
    <div class="content-wrapper">
    

<div class="row form-group mb-4">
 <div class="col">
     <label for="Name">
        Name
     </label>
     <input type="text" class="form-control" id="Name" name="Name" value="" placeholder="Name" required>

 </div>

 <div class="col">
    <label for="Prix">
     Prix
    </label>
    <input type="text" class="form-control" id="Prix" name="Prix" value="" placeholder="Prix" required>

</div>
</div>
<div class="row form-group mb-4">

<div class="col">
    <label for="Category">
        Boutique
    </label>
    <select class="form-control" id="Category" name="Category" required>
      <option value="" selected disabled>Boutique</option>
     <% categorys.forEach(element => { %>
      
    
          <option value=<%= element.name %>><%= element.name %></option>
          <% }) %>
  </select>
   
</div>







<div class="col">
   <label for="Stock">
       Stock
   </label>
   <input type="text" class="form-control" id="Stock" name="Stock" value="" placeholder="Stock" required>

</div>
</div>


<div class="form-group mb-4">
<button type="submit" class="btn btn-primary">Ajouter produit</button>
</div>
   
    </div>


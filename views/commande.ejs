<tbody>

    <% commandes.forEach(element => { %>
     <tr class="align-middle">
         <td><%= element._id %></td>
         <td><%= element.email %></td>
         <td><%= element.prenom %> <%= element.name %> </td>
         <td><%= element.createdAt.toLocaleDateString("fr-FR", {
          year:"numeric",
          month:"long",
          day:"numeric"
         }) %></td>
         <td><%= element.category %></td>
         <td class="tex-end">
              
             <div class="d-flex flex-row justify-content-end gap-2">
             
                  <a href="/historique/<%= element._id %>" type="button" class="btn btn-primary btn-small">
                  <i class="bi bi-eye"></i>
                 </a>
             </div>

         </td>



     </tr>
    <% }) %> 
 </tbody>
</table> 
